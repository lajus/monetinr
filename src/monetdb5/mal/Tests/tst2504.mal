# The prototypical factory construct
# test factory return and restart

factory random(seed:int):int;
	random:= seed;
	i:= seed;
	i:= 125 *i;
	i:= i % 32767;
	yield i;
	i:= 125 *i;
	i:= i % 32767;
	yield i;
end random;

